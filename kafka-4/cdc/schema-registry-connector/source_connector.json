{
  "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
  "tasks.max": "1",
  "database.hostname": "host.docker.internal",
  "database.port": "5442",
  "database.user": "postgres",
  "database.password": "postgres",
  "database.dbname": "source_db",
  "database.server.name": "source_postgres",
  "schema.include.list": "public",
  "topic.prefix": "postgres_cdc",
  "table.whitelist": "*",
  "key.converter": "io.confluent.connect.avro.AvroConverter",
  "key.converter.schema.registry.url": "http://host.docker.internal:8085",
  "value.converter": "io.confluent.connect.avro.AvroConverter",
  "value.converter.schema.registry.url": "http://host.docker.internal:8085",
  "transforms": "route",
  "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
  "transforms.route.regex": "([^.]+)\\.([^.]+)\\.([^.]+)",
  "transforms.route.replacement": "$3",
  "plugin.name": "pgoutput",
  "publication.name": "dbz_publication",
  "slot.name": "dbz_replication_slot",
  "heartbeat.interval.ms": "5000"
}
